.
 
\end_layout

\begin_layout Subsection*

\series medium
כאשר 
\begin_inset Formula $a=\frac{9}{10}$
\end_inset

, מתקיים 
\begin_inset Formula $\sum_{k=0}^{log(n)}\left(a+\frac{1}{10}\right)^{k}=\sum_{k=0}^{log(n)}\left(1\right)^{k}=\frac{log(n)\left(log(n)+1\right)}{2}$
\end_inset

.
 לכן:
\begin_inset Formula 
\[
n\cdot\frac{\left\lfloor log_{\max\{10,\frac{1}{a}\}}(n)\right\rfloor \left(\left\lfloor log_{\max\{10,\frac{1}{a}\}}\right\rfloor (n)+1\right)}{2}\leq n\cdot\sum_{k=0}^{m}n\cdot\left(a+\frac{1}{10}\right)^{k}\leq T(n)
\]

\end_inset


\begin_inset Formula 
\[
T(n)\leq n\cdot\sum_{k=0}^{M}\left(a+\frac{1}{10}\right)^{k}\leq n\cdot\frac{\lceil log_{\min\{10,\frac{1}{a}\}}(n)\rceil\left(\lceil log_{\min\{10,\frac{1}{a}\}}(n)\rceil(n)+1\right)}{2}
\]

\end_inset


\begin_inset Formula 
\[
n\cdot\frac{\left\lfloor log_{\max\{10,\frac{1}{a}\}}(n)\right\rfloor \left(\left\lfloor log_{\max\{10,\frac{1}{a}\}}\right\rfloor (n)+1\right)}{2}\leq T(n)\leq n\cdot\frac{\lceil log_{\min\{10,\frac{1}{a}\}}(n)\rceil\left(\lceil log_{\min\{10,\frac{1}{a}\}}(n)\rceil+1\right)}{2}
\]

\end_inset

 ולכן 
\begin_inset Formula $T(n)=\Theta(n\cdot log^{2}(n))$
\end_inset


