\begin_layout Subsection*

\series medium
\begin_inset Formula $nlog(n)$
\end_inset

: עבור 
\begin_inset Formula $n_{0}=1,c=1$
\end_inset

 לכל 
\begin_inset Formula $n\geq n_{0}$
\end_inset

 מתקיים:
\begin_inset Formula 
\[
log(n!)=log(1\cdot2\cdot\ldots\cdot n)=log(1)+log(2)+\ldots+log(n)
\]

\end_inset

 
\begin_inset Formula $log(n)$
\end_inset

 היא פונקציה עולה ולכן לכל 
\begin_inset Formula $1\leq k\leq n$
\end_inset

 מתקיים 
\begin_inset Formula $log(k)\leq log(n)$
\end_inset

.
 כלומר:
\begin_inset Formula 
\[
log(1)+\ldots+log(n)\leq log(n)+\ldots+log(n)=nlog(n)
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow log(n!)\leq1\cdot nlog(n)
\]

\end_inset

 ולכן 
\begin_inset Formula $log(n!)=O(nlog(n))$
\end_inset

.
 עבור 
\begin_inset Formula $c=\frac{1}{4}$
\end_inset

 ו
\begin_inset Formula $n_{0}\geq4$
\end_inset

 לכל 
\begin_inset Formula $n\geq n_{0}$
\end_inset

 מתקיים:
\begin_inset Formula 
\[
log(n!)=log(1)+\ldots+log\left(\frac{n}{2}\right)+\ldots+logn(n)\geq log\left(\frac{n}{2}\right)+\ldots+log(n)
\]

\end_inset


\begin_inset Formula 
\[
\geq log\left(\frac{n}{2}\right)+\ldots+log\left(\frac{n}{2}\right)=\frac{n}{2}log\left(\frac{n}{2}\right)=\frac{n}{2}\left(log(n)-log(2)\right)=\frac{n}{2}\left(log(n)-1\right)
\]

\end_inset


\begin_inset Formula 
\[
=\frac{n}{2}log(n)-\frac{n}{2}=\frac{n}{4}log(n)+\frac{n}{4}log(n)-\frac{n}{4}\cdot2=\frac{n}{4}log(n)+\frac{n}{4}\left(log(n)-2\right)\underset{n\geq4}{\underbrace{\geq}}\frac{1}{4}nlog(n)
\]

\end_inset

 כפי שנראה בתרגול.
