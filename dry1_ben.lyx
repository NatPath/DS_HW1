, שלפי הנחת האינדוקציה מקיים 
\end_layout

\begin_layout Subsection*

\series medium
\begin_inset Formula $\sum_{i=1}^{2^{k}}n'_{i}=n\cdot\left(a+\frac{1}{10}\right)^{k}$
\end_inset

 ולכן סכום השורה ה
\begin_inset Formula $k+1$
\end_inset

 הוא:
\begin_inset Formula 
\[
\sum_{i=1}^{2^{k}}a\cdot n'_{i}+\frac{1}{10}n'_{i}=\left(a+\frac{1}{10}\right)\sum_{i=1}^{2^{k}}n'_{i}=\left(a+\frac{1}{10}\right)\cdot n\cdot\left(a+\frac{1}{10}\right)^{k}=n\cdot\left(a+\frac{1}{10}\right)^{k+1}
\]

\end_inset

 כנדרש.
\end_layout

\begin_layout Subsection*

\series medium
בסך הכל, 
\begin_inset Formula $T(n)$
\end_inset

 שווה לסכום כל הרמות.
 ניתן למצוא חסם מלעיל ומלרע ל
\begin_inset Formula $T(n)$
\end_inset

 שכן סכום כל רמה בנפרד הוא חיובי, ומספר הרמות תלוי בגובה העץ.
 הקריאות הרקורסיביות הן מהצורה 
\begin_inset Formula $T(a\cdot n)$
\end_inset

 ו 
\begin_inset Formula $T\left(\frac{n}{10}\right)$
\end_inset

 ולכן גובה העץ תחום בין 
\begin_inset Formula $m=\min\left\{ log_{10}(n),log_{\frac{1}{a}}(n)\right\} $
\end_inset

 
\end_layout

\begin_layout Subsection*

\series medium
ו 
\begin_inset Formula $M=\max\left\{ log_{10}(n),log_{\frac{1}{a}}(n)\right\} $
\end_inset

.
 לכן:
\begin_inset Formula 
\[
\sum_{k=0}^{m}n\cdot\left(a+\frac{1}{10}\right)^{k}\leq T(n)\leq\sum_{k=0}^{M}n\cdot\left(a+\frac{1}{10}\right)^{k}
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow n\cdot\sum_{k=0}^{m}\left(a+\frac{1}{10}\right)^{k}\leq T(n)\leq n\cdot\sum_{k=0}^{M}\left(a+\frac{1}{10}\right)^{k}
\]

\end_inset

 נבדוק עבור אילו ערכי 
\begin_inset Formula $a$
\end_inset

 מתקיים 
\begin_inset Formula $T(n)=\omega(n)$
\end_inset

, כלומר 
\begin_inset Formula $\lim_{n\rightarrow\infty}\frac{n}{T(n)}=0$
\end_inset

.
\begin_inset Formula 
\[
\frac{1}{\sum_{k=0}^{m}\left(a+\frac{1}{10}\right)^{k}}\leq\frac{n}{T(n)}\leq\frac{1}{\sum_{k=0}^{M}\left(a+\frac{1}{10}\right)^{k}}
\]

\end_inset

 נשים לב כי כיוון ש - 
\begin_inset Formula $m$
\end_inset

 ו -
\begin_inset Formula $M$
\end_inset

 הם לוגריתמים בבסיסים שונים של 
\begin_inset Formula $n$
\end_inset

, מתקיים 
\begin_inset Formula 
\[
\lim_{n\rightarrow\infty}\sum_{k=0}^{m}\left(a+\frac{1}{10}\right)^{k}=\lim_{n\rightarrow\infty}\sum_{k=0}^{M}\left(a+\frac{1}{10}\right)^{k}=\sum_{k=0}^{\infty}\left(a+\frac{1}{10}\right)^{k}
\]

\end_inset

 ולכן מכלל הסנדוויץ' מתקיים 
\begin_inset Formula $\lim_{n\rightarrow\infty}\frac{n}{T(n)}=\lim_{n\rightarrow\infty}\frac{1}{\sum_{k=0}^{m}\left(a+\frac{1}{10}\right)^{k}}$
\end_inset

.
 
\end_layout

\begin_layout Subsection*

\series medium
